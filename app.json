{
  "expo": {
    "name": "Shraddha Infosystems",
    "slug": "Shradhha_Infinite_App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ShraddhaInfosystems.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/ShraddhaInfosystems.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.karan.m.Shradhha-Infinite-App"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/ShraddhaInfosystems.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.karan.m.Shradhha_Infinite_App",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/ShraddhaInfosystems.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-media-library",
        {
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "87956620-5f6d-4feb-a9f6-67161dabb19f"
      }
    },
    "owner": "karan.m"
  }
}
